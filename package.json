{
  "name": "hotel-guide",
  "version": "1.0.0",
  "description": "Starting project hotel guide",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "build": "grunt build && npm run makedir && npm run terser",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "makedir": "mkdir ./dist/js",
    "terser": "terser node_modules/jquery/dist/jquery.min.js node_modules/popper.js/dist/popper.min.js node_modules/bootstrap/dist/js/bootstrap.min.js node_modules/bootstrap/dist/js/bootstrap.bundle.min.js js/index.js > dist/js/main.js --compress --mangle",
    "usemin": "usemin contact.html -d dist -o dist/contact.html --htmlmin true && usemin about.html -d dist --o dist/about.html --htmlmin true && usemin index.html --d dist -o dist/index.html --htmlmin true && usemin precios.html -d dist -o dist/precios.html --htmlmin true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lordwarmax/hotel-guide.git"
  },
  "author": "Max Carmona",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lordwarmax/hotel-guide/issues"
  },
  "homepage": "https://github.com/lordwarmax/hotel-guide#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-cli": "^6.26.0",
    "copyfiles": "^2.3.0",
    "cssmin": "^0.4.3",
    "grunt": "^1.2.1",
    "grunt-babel": "^8.0.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-rollup": "^11.2.0",
    "grunt-terser": "^1.0.0",
    "grunt-usemin": "^3.1.1",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.5.4",
    "node-sass": "^4.9.3",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "terser": "^4.8.0",
    "terser-webpack-plugin": "^3.0.6",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.2.0",
    "jquery": "^3.5.1",
    "minimist": "^1.2.5",
    "node": "^14.5.0",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "sass": "^1.26.10"
  }
}
